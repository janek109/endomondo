{
    "name": "jszewczak/endomondo",
    "type": "project",
    "authors": [
        {
            "name": "Jan Szewczak",
            "email": "janek109@poczta.fm"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "fabulator/endomondo-api": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "iamstuartwilson/strava": "^1.4",
        "myclabs/php-enum": "^1.7",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "EndomondoMv\\": "src/",
            "Swagger\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EndomondoMv\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
